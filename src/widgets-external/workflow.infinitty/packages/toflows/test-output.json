    {
      "id": "skill-build-ai-agents",
      "type": "skill",
      "position": {
        "x": -475,
        "y": 180
      },
      "data": {
        "label": "build-ai-agents",
        "description": "Build AI agents with Vercel AI SDK from scratch through shipping. Full lifecycle - build, debug, test, optimize, ship. Covers streamText, generateText, useChat, AI Elements, multi-turn tool calling, and agent loops.",
        "nodeType": "skill",
        "principleCount": 4,
        "workflowCount": 7,
        "referenceCount": 10
      }
    },
    {
      "id": "skill-build-ai-agents-principles",
      "type": "group",
      "position": {
        "x": -475,
