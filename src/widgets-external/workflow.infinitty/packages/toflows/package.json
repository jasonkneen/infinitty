{
  "name": "toflows",
  "version": "0.1.0",
  "description": "Transform structured data (skills, schemas, workflows) into Mermaid diagrams with ReactFlow support",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "toflows": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts"
  },
  "keywords": [
    "reactflow",
    "graph",
    "visualization",
    "skills",
    "workflows"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^22.10.2",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "dependencies": {
    "glob": "^11.0.0",
    "gray-matter": "^4.0.3",
    "commander": "^12.1.0",
    "elkjs": "^0.9.3"
  }
}
